{
  "name": "use-google-autocomplete-input",
  "version": "1.0.0",
  "description": "Hook to add Google Places Autocomplete to an input",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "./lib",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./esm/index.js",
      "require": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "rm -rf ./lib/* ./esm/* && yarn build:cjs && yarn build:es",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "prepublishOnly": "yarn build"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "author": "Bruno Felipe",
  "license": "MIT",
  "keywords": [
    "google-places",
    "react-google-autocomplete",
    "react-autocomplete",
    "google-autocomplete",
    "use-google-autocomplete-input"
  ],
  "files": [
    "esm",
    "lib"
  ],
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.11.4"
  },
  "devDependencies": {
    "@types/google.maps": "^3.44.6",
    "@types/node": "^15.3.1",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
